{
  "name": "socket.io-mock",
  "version": "1.2.4",
  "description": "a mocked version of a socket.io socket for testing",
  "main": "dist/index.min.js",
  "module": "dist/index.js",
  "scripts": {
    "lint": "standard",
    "test": "./scripts/test.sh",
    "test-server": "./scripts/test-server.sh",
    "test-browser": "./scripts/test-browser.sh",
    "build": "./scripts/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supremetechnopriest/socket.io-mock.git"
  },
  "keywords": [
    "socket.io",
    "test",
    "mock",
    "socket",
    "mocha",
    "chai",
    "karma"
  ],
  "author": "Randy Lebeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supremetechnopriest/socket.io-mock/issues"
  },
  "homepage": "https://github.com/supremetechnopriest/socket.io-mock#readme",
  "dependencies": {
    "component-emitter": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-watch": "^4.3.1",
    "standard": "^14.3.1",
    "uglify-js": "^3.8.0",
    "wrtc": "0.4.4"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "mocha": {
