{
  "name": "socket.io-mock",
  "version": "1.0.1",
  "description": "a mocked version of a socket.io socket for testing",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && yarn lint && yarn test && vite build",
    "build:skip": "rimraf dist && vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "tsc": "tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moodiest/socket.io-mock.git"
  },
  "keywords": [
    "socket.io",
    "socket",
    "test",
    "mock",
    "mocked"
  ],
  "author": "James",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moodiest/socket.io-mock/issues"
  },
  "homepage": "https://github.com/moodiest/socket.io-mock#readme",
  "dependencies": {
    "@types/component-emitter": "^1.2.11",
    "component-emitter": "^1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-checker": "^0.4.4",
    "vite-plugin-dts": "^1.0.5",
    "vitest": "^0.8.3"
  }
}
