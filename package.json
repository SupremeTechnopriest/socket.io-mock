{
  "name": "socket.io-mock",
  "version": "1.2.0",
  "description": "a mocked version of a socket.io socket for testing",
  "main": "dist/index.min.js",
  "module": "dist/index.js",
  "scripts": {
    "lint": "./scripts/lint.sh",
    "test": "./scripts/test.sh",
    "test-server": "./scripts/test-server.sh",
    "test-browser": "./scripts/test-browser.sh",
    "build": "./scripts/build.sh"
  },
  "xo": {
    "parser": "babel-eslint",
    "env": [
      "es6",
      "node",
      "browser",
      "mocha"
    ],
    "globals": [
      "chai",
      "assert",
      "expect"
    ],
    "ignores": [
      "test/**",
      "dist/**"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "array-bracket-spacing": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supremetechnopriest/socket.io-mock.git"
  },
  "keywords": [
    "socket.io",
    "test",
    "mock",
    "socket",
    "mocha",
    "chai",
    "karma"
  ],
  "author": "Randy Lebeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supremetechnopriest/socket.io-mock/issues"
  },
  "homepage": "https://github.com/supremetechnopriest/socket.io-mock#readme",
  "dependencies": {
    "component-emitter": "^1.2.1",
    "debug": "^2.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-rollup-plugin": "^0.2.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-bundle-size": "^1.0.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "uglify-js": "^2.7.5",
    "wrtc": "0.0.61",
    "xo": "^0.17.1"
  }
}
