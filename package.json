{
  "name": "socket.io-mock",
  "version": "1.3.1",
  "description": "a mocked version of a socket.io socket for testing",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "standard",
    "test": "./scripts/test.sh",
    "build": "./scripts/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supremetechnopriest/socket.io-mock.git"
  },
  "keywords": [
    "socket.io",
    "test",
    "mock",
    "socket",
    "mocha",
    "chai",
    "karma"
  ],
  "author": "Randy Lebeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supremetechnopriest/socket.io-mock/issues"
  },
  "homepage": "https://github.com/supremetechnopriest/socket.io-mock#readme",
  "dependencies": {
    "emittery": "^0.9.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "standard": "^14.3.4",
    "uglify-js": "^3.10.0"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "mocha": {
